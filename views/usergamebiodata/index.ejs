<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Game BIodata</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
  <a href="/view/usergamebiodata/create" class="btn btn-success m-3">Create</a>
  
  <a href="usergames" class="btn btn-success m-3">User Game</a>

  <a href="usergamebiodata" class="btn btn-success m-3">Biodata</a>

  <a href="usergamehistory" class="btn btn-success m-3">History</a>

  <a href="/logout" ><button class="btn btn-danger btn-sm">LOGOUT</button></a>

  <table class="table table-striped table-sm m-3" >
    <thead>
      <tr>
        <th scope="col">No</th>
        <th scope="col">Nama</th>
        <th scope="col">Email</th>
        <th scope="col">Gender</th>
        <th scope="col">Created At</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
               <% usergamebiodata.forEach((usergamebiodata,index) => { %>
      <tr>
        <td><%= index+1 %></td>
        <td><%= usergamebiodata.nama %></td>
        <td><%= usergamebiodata.email %></td>
        <td><%= usergamebiodata.gender %></td>
        <td><%= moment(usergamebiodata.createdAt).format('Do MMMM, YYYY') %></td>
        <td>
          <a href="/view/usergamebiodata/update/<%= usergamebiodata.id %>"><button class="btn btn-success btn-sm">Edit Data</button></a>
          <a href="/view/usergamebiodata/delete/<%= usergamebiodata.id %>"><button class="btn btn-danger btn-sm">Delete Data</button></a>
        </td>
      </tr>
      <% }) %> 

    </tbody>
  </table>

</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</html>